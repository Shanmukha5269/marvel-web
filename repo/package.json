{
  "private": true,
  "name": "uvcemarvel",
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-ui": "turbo run build --filter ui && changeset version && changeset publish && npm publish --prefix ./packages/ui"
  },
  "packageManager": "pnpm@7.15.0",
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@manypkg/cli": "0.20.0",
    "@storybook/cli": "^7.0.9",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "eslint-config-custom": "0.0.0",
    "next": "^13.4.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "turbo": "^1.9.3"
  }
}
